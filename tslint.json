{
  "extends": "tslint:latest",
  "rules": {
    "indent": [
      true,
      2
    ],
    "no-trailing-whitespace": false,
    "arrow-parens": false,
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      false
    ],
    "no-string-literal": false,
    "ordered-imports": [
      false
    ],
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "member-access": [
      false
    ],
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "curly": true,
    "class-name": true,
    "semicolon": [
      "always"
    ],
    "triple-equals": true
  }
}