{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "es2015",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../../dist/@company/skel",
    "rootDir": ".",
    "sourceMap": true,
    "target": "es5",
    "inlineSources": true,
    "stripInternal": false,
    "baseUrl": ".",
    "paths": {},
    "typeRoots": [
      "../../node_modules/@types"
    ],
    "types": [
      "systemjs",
      "jasmine",
      "rx",
      "node"
    ],
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "angularCompilerOptions": {
    "genDir": "../../dist/@company/skel",
    "skipTemplateCodegen": true,
    "debug": true
  }
}